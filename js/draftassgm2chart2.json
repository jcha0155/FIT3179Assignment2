{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Comparison between Homicide Death Rates and Unemployment Rates ",

    "width": 800,
    "height": 500,
    "data": {
      "url": "https://raw.githubusercontent.com/jcha0155/3179/main/FIT3179%20Assignment%202/data/datahomicideandunemployment.csv"
  },
  "mark": {
    "type": "line",
    "point":{

      "filled": false,
      "fill": "white"

    }

},
  "encoding": {
  "x": {"field": "Year", "type": "ordinal"},
  "y": {
    "field": "Unemployment_Rate",
    "type": "quantitative",
    "title": "Unemployment Rate (% of total labor force)"
    
  },
  "color": {
    "field": "Country", 
    "type": "nominal"
    
  },
  "tooltip": [
    {"field": "Unemployment_Rate", "type": "quantitative", "title": "Unemployment Rate (% of total labor force)"},
    {"field": "Country", "type": "nominal", "title": "Country"}
    
  ]

  },
  "params": [
    {
      "name": "Country",
      "bind":{

        "input":"select",
        "options":[
          null,
          "Argentina",
          "Bolivia",
          "Brazil",
          "Chile",
          "Colombia",
          "Ecuador",
          "Guyana",
          "Paraguay",
          "Peru",
          "Suriname",
          "Uruguay",
          "Venezuela"
        ],
        "labels": [
          "Show All",
          "Argentina",
          "Bolivia",
          "Brazil",
          "Chile",
          "Colombia",
          "Ecuador",
          "Guyana",
          "Paraguay",
          "Peru",
          "Suriname",
          "Uruguay",
          "Venezuela"
        ],
        "name": "Country"
      }
    }

  ],
  "transform":[

    {"filter": "Country == null || datum.Country == Country"}



  ]
  
  
  
}